{
  "name": "ts-isometric-map",
  "version": "1.0.0",
  "description": "Isometric map made with Rust and HTML5 Canvas",
  "main": "index.js",
  "scripts": {
    "wasm:bindgen:build": "./zx-wasm-build.mjs --bindgen",
    "wasm:pack:build": "./zx-wasm-build.mjs --pack",
    "wasm:app": "npm init wasm-app -w packages/web-isomap",
    "build": "npm run wasm:pack:build; npm run build -w=web-isomap",
    "start": "npm start -w=web-isomap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bsorrentino/ts-isometric-map.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bsorrentino/ts-isometric-map/issues"
  },
  "homepage": "https://github.com/bsorrentino/ts-isometric-map#readme",
  "workspaces": [
    "modules/wasm-isomap",
    "modules/web-isomap"
  ],
  "devDependencies": {
    "zx": "^6.1.0"
  }
}
